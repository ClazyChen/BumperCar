entity angleAdd is
	port(
		clk: in std_logic; --50MHz
		sin1, cos1, sin2, cos2: in std_logic_vector(31 downto 0); --输入的两个角
		sino, coso: out std_logic_vector(31 downto 0); --输出的角
	);
end angleAdd;
